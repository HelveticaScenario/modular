{
    "carrier-osc": {
        "module_type": "sine-oscillator",
        "params": {
            "freq": {
                "param_type": "note",
                "value": 69
            }
        }
    },
    "lfo": {
        "module_type": "sine-oscillator",
        "params": {
            "freq": {
                "param_type": "note",
                "value": 0
            }
        }
    },
    "lfo-attenuator": {
        "module_type": "scale-and-shift",
        "params": {
            "input": {
